{
  "name": "forecasting-service",
  "version": "1.0.0",
  "description": "An Express.js template with TypeScript, ESLint and Prettier",
  "imports": {
    "#*": "./src/*"
  },
  "scripts": {
    "dev": "tsx --watch --env-file .env.dev src/index.ts",
    "start": "node --env-file .env.dev dist/index.js",
    "build": "tsc -p tsconfig.build.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui",
    "coverage": "vitest --run coverage",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Godlisten-Ngatara/express-typescript-eslint-prettier-template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Godlisten-Ngatara/express-typescript-eslint-prettier-template/issues"
  },
  "homepage": "https://github.com/Godlisten-Ngatara/express-typescript-eslint-prettier-template#readme",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,yml,yaml,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@types/papaparse": "^5.3.16",
    "bullmq": "^5.58.5",
    "express": "^5.1.0",
    "ioredis": "^5.7.0",
    "papaparse": "^5.5.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.0",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/eslint-plugin": "^1.2.7",
    "@vitest/ui": "^3.2.3",
    "eslint": "^9.29.0",
    "eslint-plugin-perfectionist": "^4.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vitest": "^3.2.3"
  }
}
